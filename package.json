{
	"name": "timeline-view",
	"version": "1.4.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"check": "svelte-check",
		"dev": "vite build --mode development --watch",
		"test:unit": "vitest --config ./test/vitest.config.ts",
		"test:view": "node component_tests",
		"dev:component": "vite -c component_tests/vite.config.js",
		"build": "npm run obsidian:version && vite build && git add styles.css versions.json manifest.json",
		"obsidian:version": "tsx src/obsidian/build/version-bump.ts && git add src/obsidian/build/versions.ts"
	},
	"type": "module",
	"module": "main.js",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"obsidian-search": "0.1.1"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "latest",
		"@tsconfig/svelte": "latest",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"builtin-modules": "latest",
		"esbuild": "latest",
		"obsidian": "latest",
		"prettier-plugin-svelte": "latest",
		"svelte": "latest",
		"svelte-check": "latest",
		"svelte-preprocess": "latest",
		"tslib": "latest",
		"tsx": "latest",
		"typescript": "latest",
		"vite": "latest",
		"vitest": "latest"
	},
	"prettier": {
		"plugins": [
			"prettier-plugin-svelte"
		],
		"tabWidth": 4,
		"useTabs": true,
		"printWidth": 120
	}
}